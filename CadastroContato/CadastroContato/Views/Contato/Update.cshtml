@{
    ViewData["Title"] = "Edição de Contato";
}
<div class="text-center">
    <h1>Edição de Contato</h1>
</div>


<form>
    <div class="form-group">
        <label for="name">Nome</label>
        <input type="text" class="form-control" placeholder="Digite o nome do contato">
    </div>
    <div class="form-group">
        <label for="email">Email</label>
        <input type="text" class="form-control" placeholder="Digite o email do contato">
    </div>
    <div class="form-group">
        <label for="cellphone">Celular</label>
        <input type="text" class="form-control" placeholder="Digite o celular do contato">
    </div>
    <button type="submit" class="btn btn-info">Salvar</button>
    <a class="btn btn-info" role="button" asp-controller="Contato" asp-action="Index">Voltar</a>
</form>